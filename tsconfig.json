{
	"compilerOptions": {
		"lib": ["ES2022", "DOM"],
		"module": "ES2022",
		"esModuleInterop": true,
		"target": "ES2022",
		"noImplicitAny": true,
		"moduleResolution": "Node",
		"sourceMap": true,
		"outDir": "dist/",
		"baseUrl": ".",
		"paths": {
				"*": [
						"node_modules/*"
				]
		}
},
"ts-node": {
	"esm": true,
	"experimentalSpecifierResolution": "node"
},
"include": [	
	"dist/**/*.ts", 
	"src/index.ts"], 
"exclude": []
}